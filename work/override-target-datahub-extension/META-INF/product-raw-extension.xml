<extension xmlns="http://www.hybris.com/schema/" name="product-raw-extension">
    <rawItems>
        <item>
            <type>RawCatalogProduct</type>
            <description>Raw representation of a product</description>
            <attributes>
                <attribute>
                    <name>sourceId</name>
                </attribute>
                <attribute>
                    <name>baseName</name>
                </attribute>
                <attribute>
                    <name>baseDescription</name>
                </attribute>
                <attribute>
                    <name>ean</name>
                </attribute>
                <attribute>
                    <name>manufacturerName</name>
                </attribute>
                <attribute>
                    <name>manufacturerAid</name>
                </attribute>
                <attribute>
                    <name>unit</name>
                </attribute>
                <attribute>
                    <name>style</name>
                </attribute>
                <attribute>
                    <name>size</name>
                </attribute>
                <attribute>
                    <name>sku</name>
                </attribute>
                <attribute>
                    <name>category</name>
                </attribute>
                <attribute>
                    <name>styleName</name>
                </attribute>
                <attribute>
                    <name>styleDescription</name>
                </attribute>
            </attributes>
        </item>
    </rawItems>
    <canonicalItems>
        <item>
            <type>CanonicalCatalogProduct</type>
            <attributes>
                <attribute>
                    <name>identifier</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>sourceId</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>name</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>baseName</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>description</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>baseDescription</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>barCode</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>ean</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>manName</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>manufacturerName</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>manAid</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>manufacturerAid</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>unit</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>unit</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>style</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>style</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>size</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>size</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>sku</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>sku</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>category</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>category</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>styleName</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>styleName</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>styleDescription</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>styleDescription</expression>
                        </transformation>
                    </transformations>
                </attribute>
            </attributes>
        </item>
    </canonicalItems>
</extension>