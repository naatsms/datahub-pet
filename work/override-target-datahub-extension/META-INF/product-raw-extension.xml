<extension xmlns="http://www.hybris.com/schema/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.hybris.com/schema/datahub-metadata-schema-1.3.0.xsd" name="product-raw-extension">

    <dependencies>
        <dependency>
            <extension>product-canonical-extension</extension>
        </dependency>
    </dependencies>

    <rawItems>
        <item>
            <type>RawCatalogProduct</type>
            <description>Full Raw representation of source product</description>
            <attributes>
                <attribute>
                    <name>sourceId</name>
                </attribute>
                <attribute>
                    <name>isoCode</name>
                </attribute>
                <attribute>
                    <name>baseName</name>
                </attribute>
                <attribute>
                    <name>baseDescription</name>
                </attribute>
                <attribute>
                    <name>ean</name>
                </attribute>
                <attribute>
                    <name>manufacturerName</name>
                </attribute>
                <attribute>
                    <name>manufacturerAid</name>
                </attribute>
                <attribute>
                    <name>unit</name>
                </attribute>
                <attribute>
                    <name>style</name>
                </attribute>
                <attribute>
                    <name>size</name>
                </attribute>
                <attribute>
                    <name>sku</name>
                </attribute>
                <attribute>
                    <name>category</name>
                </attribute>
                <attribute>
                    <name>styleName</name>
                </attribute>
                <attribute>
                    <name>styleDescription</name>
                </attribute>
            </attributes>
        </item>
    </rawItems>

    <canonicalItems>
        <item>
            <type>CanonicalCatalogProduct</type>
            <attributes>
                <attribute>
                    <name>id</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>sourceId</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>name</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>baseName</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>description</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>baseDescription</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>ean</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>manufacturer</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                            <expression>manufacturerName</expression>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>unit</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>style</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>size</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                        </transformation>
                    </transformations>
                </attribute>
                <attribute>
                    <name>category</name>
                    <transformations>
                        <transformation>
                            <rawSource>RawCatalogProduct</rawSource>
                        </transformation>
                    </transformations>
                </attribute>
            </attributes>
        </item>
    </canonicalItems>

</extension>